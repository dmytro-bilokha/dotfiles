
XXkb.xpm.path: /usr/local/share/xxkb

XXkb.group.base: 1
XXkb.group.alt: 2

XXkb.mainwindow.enable: yes
XXkb.mainwindow.type: tray
XXkb.mainwindow.in_tray: no
XXkb.mainwindow.appicon: no
XXkb.mainwindow.geometry: 16x16+0+0
XXkb.mainwindow.xpm.1: en15.xpm
XXkb.mainwindow.xpm.2: ru15.xpm
XXkb.mainwindow.xpm.3: ua15.xpm
XXkb.mainwindow.xpm.4:

! XXkb.mainwindow.in_tray: GNOME
! possible values - KDE, KDE2, GNOME

XXkb.button.enable: no
XXkb.button.geometry: 16x16+0+0
XXkb.button.gravity: NorthEast
XXkb.button.xpm.1: en15.xpm
XXkb.button.xpm.2: ru15.xpm
XXkb.button.xpm.3: ua15.xpm
XXkb.button.xpm.4:

XXkb.mainwindow.label.enable: yes
XXkb.mainwindow.label.font: -misc-*-r-*-14-*
XXkb.mainwindow.label.background: black
XXkb.mainwindow.label.foreground: gray
XXkb*label.text.1: EN
XXkb*label.text.2: RU
XXkb*label.text.3: UA


XXkb.controls.add_when_start: yes
XXkb.controls.add_when_create: yes
XXkb.controls.add_when_change: yes
XXkb.controls.focusout: no
XXkb.controls.two_state: no
XXkb.controls.button_delete: yes
XXkb.controls.button_delete_and_forget: no
XXkb.controls.mainwindow_delete: yes

XXkb.mousebutton.1.reverse: no
XXkb.mousebutton.3.reverse: no

XXkb.bell.enable: no
XXkb.bell.percent: -50

XXkb.ignore.reverse: no

! XXkb.app_list.<match>.<action>: <list>
! <match> is one of "wm_class_class", "wm_class_name", "wm_name", "property"
! <action> is one of "ignore", "start_alt", "alt_groupM" (M - 1..4) 
! For example:
!   XXkb.app_list.wm_class_name.ignore: "Gnome-panel"
!   XXkb.app_list.wm_class_class.ignore: "Gnome-panel"
!
!  ignore windows in KDE tray
!  XXkb.app_list.property.ignore: _KDE_NET_WM_SYSTEM_TRAY_WINDOW_FOR
  XXkb.app_list.property.ignore: _NET_WM_ACTION_CHANGE_DESKTOP
